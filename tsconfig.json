{
    "compilerOptions": {
       "lib": [
          "es5",
          "es6"
       ],
       "target": "es5",
       "module": "commonjs",
       "moduleResolution": "node",
       "outDir": "./dist",
       "skipLibCheck": true,
       "declaration": true,
       "strictPropertyInitialization": false,
       "emitDecoratorMetadata": true,
       "experimentalDecorators": true,
       "strict": true,
       "sourceMap": true,
       "resolveJsonModule": true,
       "esModuleInterop": true
    },
    "exclude": [
       "node_modules",
       "dist"
    ]
  }
  